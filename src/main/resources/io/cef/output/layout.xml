<beanio xmlns="http://www.beanio.org/2012/03">
    <stream name="basic" format='csv'>
        <parser>
            <property name="delimiter" value=";"/>
        </parser>

        <record name="header" order="1" occurs="1">
            <field name="id" default="Id da proposta"/>
            <field name="proposalNumber" default="Numero da proposta"/>
            <field name="cpfCnpj" default="CPF ou CNPJ"/>
            <field name="fantasyName" default="Nome Fantasia"/>
            <field name="socialReason" default="Razao Social"/>
            <field name="voucherName" default="Nome Plaqueta (Comprovante)"/>
            <field name="userId" default="UserId"/>
            <field name="agentCpfCnpj" default="CPF ou CNPJ do agente"/>
            <field name="institution" default="Instituicao"/>
            <field name="serviceContract" default="Service Contract"/>

            <field name="optIn" default="Opt In"/>
            <field name="sellerRegistration" default="Matricula do Vendedor"/>

            <field name="subChannel" default="Sub Canal"/>

            <field name="tecnology" default="Tecnologia"/>
            <field name="includeDate" default="Inclusao em"/>
            <field name="conclusionDate" default="Conclusao em"/>
            <field name="merchantId" default="Merchant ID"/>
            <field name="fiservStatus" default="Status - FISERV"/>
            <field name="cefStatus" default="Status - CAIXA"/>
            <field name="cefMessage" default="Mensagem Caixa"/>
            <field name="error" default="Erros"/>
            <field name="submissionDate" default="Submissao ao Online em"/>
        </record>

        <record name="basic" order="2" occurs="0+"
                class="com.fiserv.preproposal.api.domain.dtos.BasicReport">

            <field name="id"/>
            <field name="proposalNumber"/>
            <field name="cpfCnpj"/>
            <field name="fantasyName"/>
            <field name="socialReason"/>
            <field name="voucherName"/>
            <field name="userId"/>
            <field name="agentCpfCnpj"/>
            <field name="institution"/>
            <field name="serviceContract"/>
            <field name="optIn"/>
            <field name="sellerRegistration"/>
            <field name="subChannel"/>
            <field name="tecnology"/>
            <field name="includeDate"/>
            <field name="conclusionDate"/>
            <field name="merchantId"/>
            <field name="fiservStatus"/>
            <field name="cefStatus"/>
            <field name="cefMessage"/>
            <field name="error"/>
            <field name="submissionDate"/>

        </record>
    </stream>

    <stream name="quantitative" minOccurs="0" maxOccurs="unbounded" format='csv'>
        <parser>
            <property name="delimiter" value=";"/>
        </parser>

        <record name="header" order="1" occurs="1">
            <field name="filename" default="Nome do Arquivo"/>
            <field name="institution" default="Instituicao"/>
            <field name="processingDate" default="Data de processamento"/>
            <field name="fileStatus" default="Status do Arquivo"/>
            <field name="proposals" default="Qtd. de Propostas"/>
            <field name="onlineRejected" default="Qtd. Rejeitado no Online"/>
            <field name="pendingInFiservOnline" default="Qtd. Pendente no Online"/>
            <field name="pendingComplement" default="Qtd. Pendente Complemento"/>
            <field name="preproposalError" default="Qtd. erro pre proposta"/>
            <field name="finished" default="Qtd. Concluida"/>
            <field name="tmpCanceled" default="Qtd. Cancelada TMP"/>
            <field name="credOnline" default="Qtd. Credenciada Online"/>
        </record>

        <record name="quantitative" order="2" occurs="0+"
                class="com.fiserv.preproposal.api.domain.dtos.QuantitativeReport">

            <field name="filename"/>
            <field name="institution"/>
            <field name="processingDate"/>
            <field name="fileStatus"/>
            <field name="proposals"/>
            <field name="onlineRejected"/>
            <field name="pendingInFiservOnline"/>
            <field name="pendingComplement"/>
            <field name="preproposalError"/>
            <field name="finished"/>
            <field name="tmpCanceled"/>
            <field name="credOnline"/>

        </record>
    </stream>

